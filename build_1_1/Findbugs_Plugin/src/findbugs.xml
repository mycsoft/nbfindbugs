<FindbugsPlugin pluginid="com.simlink.fw.findbugs.plugin"
                defaultenabled="true"
                provider="Simlink Framework" >
    <Detector class="com.simlink.fw.findbugs.plugin.FindServiceSQL" speed="fast" />
    <BugPattern abbrev="SLBG" type="SIMLINK_SERVICE_SQL" category="CORRECTNESS" />
    
    <Detector class="com.simlink.fw.findbugs.plugin.FindSysOutPrint" speed="fast" />
    <BugPattern abbrev="SLBG" type="SIMLINK_SYSTEM_OUT_PRINT" category="CORRECTNESS" />
    
    <Detector class="com.simlink.fw.findbugs.plugin.FindExceptionPrint" speed="fast" />
    <BugPattern abbrev="SLBG" type="SIMLINK_EXCEPTION_PRINT" category="CORRECTNESS" />
</FindbugsPlugin>

