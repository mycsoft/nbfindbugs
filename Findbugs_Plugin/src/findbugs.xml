<FindbugsPlugin pluginid="com.spsoft.findbugs.plugin"
                defaultenabled="true"
                provider="SPSOFT Framework" >
    <Detector class="com.spsoft.findbugs.plugin.FindServiceSQL" speed="fast" />
    <BugPattern abbrev="SPBG" type="SPSOFT_SERVICE_SQL" category="CORRECTNESS" />
    
    <Detector class="com.spsoft.findbugs.plugin.FindSysOutPrint" speed="fast" />
    <BugPattern abbrev="SPBG" type="SPSOFT_SYSTEM_OUT_PRINT" category="CORRECTNESS" />
    
    <Detector class="com.spsoft.findbugs.plugin.FindExceptionPrint" speed="fast" />
    <BugPattern abbrev="SPBG" type="SPSOFT_EXCEPTION_PRINT" category="CORRECTNESS" />

    <Detector class="com.spsoft.findbugs.plugin.FindAccError" speed="moderate" />
    <BugPattern abbrev="SPBG" type="SPSOFT_PRJACC_UNSUMMED" category="CORRECTNESS" />

    <Detector class="com.spsoft.findbugs.plugin.FindMoreReturn" speed="fast" />
    <BugPattern abbrev="SPBG" type="SPSOFT_MORE_RETURN" category="CORRECTNESS" />

    <Detector class="com.spsoft.findbugs.plugin.FindThrowException" speed="fast" />
    <BugPattern abbrev="SPBG" type="SPSOFT_ERROR_EXCEPTION" category="CORRECTNESS" />

    <Detector class="com.spsoft.findbugs.plugin.FindMoreLoop" speed="fast" />
    <BugPattern abbrev="SPBG" type="SPSOFT_MORE_LOOP" category="CORRECTNESS" />

    <Detector class="com.spsoft.findbugs.plugin.FindInvokeSqlInLoop" speed="moderate" />
    <BugPattern abbrev="SPBG" type="SPSOFT_LOOP_SQL" category="PERFORMACE" />
</FindbugsPlugin>

